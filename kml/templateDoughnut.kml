<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2" xmlns:gx="http://www.google.com/kml/ext/2.2" xmlns:kml="http://www.opengis.net/kml/2.2" xmlns:atom="http://www.w3.org/2005/Atom">
<Document>
	<name>{{tag}}</name>
	<StyleMap id="msn_ylw-pushpin">
		<Pair>
			<key>normal</key>
			<styleUrl>#sn_ylw-pushpin</styleUrl>
		</Pair>
		<Pair>
			<key>highlight</key>
			<styleUrl>#sh_ylw-pushpin</styleUrl>
		</Pair>
	</StyleMap>
	<Style id="sn_ylw-pushpin">
		<LineStyle>
			<color>
				{{lineColour}}
				</color>
		</LineStyle>
		<PolyStyle>
			<color>
				{{polyColour}}
			</color>
		</PolyStyle>
	</Style>
	<Style id="sh_ylw-pushpin">
		<IconStyle>
			<scale>1.2</scale>
		</IconStyle>
		<LineStyle>
			<color>
				{{lineColour}}
			</color>
		</LineStyle>
		<PolyStyle>
			<color>
				{{polyColour}}
			</color>
		</PolyStyle>
	</Style>
	<Placemark>
		<name>
			{{tag}}
		</name>
		<styleUrl>#msn_ylw-pushpin</styleUrl>
		<Polygon>
			<extrude>1</extrude>
			<gx:altitudeMode>clampToSeaFloor</gx:altitudeMode>
			<outerBoundaryIs>
				<LinearRing>
					<coordinates>
						{{outCoordinates}}
					</coordinates>
				</LinearRing>
			</outerBoundaryIs>
			<innerBoundaryIs>
				<LinearRing>
					<coordinates>
						{{inCoordinates}}
					</coordinates>
				</LinearRing>
			</innerBoundaryIs>
		</Polygon>
	</Placemark>
</Document>
</kml>
